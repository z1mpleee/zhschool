<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqz.zhschool.mapper.StudentMapper">

    <!--public Student selectAllByNameAndPassword(String name, String password);-->
    <select id="selectAllByNameAndPassword" resultType="com.cqz.zhschool.pojo.Student">
        SELECT * FROM t_student WHERE name = #{name} AND password = #{password}
    </select>
</mapper>